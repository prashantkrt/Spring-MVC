<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Form for Implementers</title>
</head>
<body style="background-color: DodgerBlue;">
	<div>
		<center>
			<h1 style="color: SlateBlue"><marquee><b>Welcome to Implementers registration page</b></marquee></h1>
			<form method="post"
				style="background-color: MediumSeaGreen; width: 350px; padding: 20px">
				<table style="height: 400px; width: 200px">
					<tr>
						<td><b>ID :</b></td>
						<td><input type="text" name="id" /></td>
					</tr>
					<tr>
						<td><b>Name :</b></td>
						<td><input type="text" name="name" /></td>
					</tr>
					<tr>
						<td><b>Age :</b></td>
						<td><input type="text" name="age" /></td>
					</tr>
					<tr>
						<td><b>Gender :</b></td>
						<td><input type="text" name="gender" /></td>
					</tr>
					<tr>
						<td><b>City :</b></td>
						<td><input type="text" name="city" /></td>
					</tr>
					<tr>
						<td><b>Salary :</b></td>
						<td><input type="text" name="salary" /></td>
					</tr>
					<tr>
						<td><input type="submit" name="submit"
							style="height: 20px; width: 100px" /></td>
					</tr>
				</table>
			</form>
	</div>
	</center>

</body>
</html>